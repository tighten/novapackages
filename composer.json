{
    "name": "tightenco/novapackages",
    "description": "Nova Packages",
    "keywords": [
        "laravel nova",
        "nova",
        "saas",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.2",
        "algolia/algoliasearch-client-php": "^3.3",
        "erusev/parsedown": "^1.7",
        "laravel/framework": "^12.53",
        "laravel/helpers": "^1.7",
        "laravel/horizon": "^5.30",
        "laravel/passport": "^13.0",
        "laravel/scout": "^10.12",
        "laravel/slack-notification-channel": "^3.4",
        "laravel/socialite": "^5.17",
        "laravel/tinker": "^2.10.1",
        "laravel/ui": "^4.6",
        "league/flysystem-aws-s3-v3": "^3.29",
        "livewire/livewire": "^3.5",
        "php-http/guzzle7-adapter": "^1.1",
        "predis/predis": "^1.1",
        "spatie/laravel-feed": "^4.4",
        "spatie/laravel-flare": "^2.7",
        "spatie/laravel-sitemap": "^7.3",
        "stil/gd-text": "^1.1",
        "tightenco/duster": "^3.3",
        "tightenco/ziggy": "^2.5",
        "typesense/typesense-php": "^4.9",
        "willvincent/laravel-rateable": "^3.5"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.15",
        "fakerphp/faker": "^1.23",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "phpunit/phpunit": "^11.5.3",
        "laravel/pail": "^1.2.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite --kill-others"
        ],
        "setup": [
            "composer install",
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php artisan key:generate",
            "@php artisan migrate --force",
            "npm install",
            "npm run build"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ],
        "pre-package-uninstall": [
            "Illuminate\\Foundation\\ComposerScripts::prePackageUninstall"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
