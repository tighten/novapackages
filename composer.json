{
    "name": "tightenco/novapackages",
    "description": "Nova Packages",
    "keywords": [
        "laravel nova",
        "nova",
        "saas",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.4|^8.0",
        "algolia/algoliasearch-client-php": "^3.1",
        "bugsnag/bugsnag-laravel": "^2.22",
        "doctrine/dbal": "^3.2.0",
        "erusev/parsedown": "^1.7",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "knplabs/github-api": "^3.0",
        "laravel/framework": "^8.80",
        "laravel/helpers": "^1.4",
        "laravel/horizon": "^5.7",
        "laravel/passport": "^10.2",
        "laravel/scout": "^9.3",
        "laravel/slack-notification-channel": "^2.3",
        "laravel/socialite": "^5.2",
        "laravel/telescope": "^4.6",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.4",
        "laravelium/sitemap": "^8.0",
        "livewire/livewire": "^2.8",
        "php-http/guzzle7-adapter": "^1.0",
        "predis/predis": "^1.1",
        "spatie/laravel-feed": "^4.0",
        "stil/gd-text": "^1.1",
        "tightenco/ziggy": "^1.4.2",
        "willvincent/laravel-rateable": "^2.4"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "codedungeon/phpunit-result-printer": "^0.31",
        "dms/phpunit-arraysubset-asserts": "^0.2.0",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
